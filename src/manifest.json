{
  "package": "com.quickapp.template",
  "name": "Quick App Template",
  "versionName": "1.0.0",
  "versionCode": 1,
  "icon": "/Common/logo.png",
  "minPlatformVersion": 1060,
  "features": [
        {
            "name": "system.webview"
        },
        {
            "name": "system"
        }
    ],
    "permissions": [
        {
            "origin": "*"
        }
    ],
    "config": {},
    "router": {
        "entry": "Hello",
        "pages": {
            "Hello": {
                "component": "hello"
            }
        }
    },
    "display": {
        "pages": {
            "Hello": {
                "titleBarText": "TitleBar"
            }
        },
        "titleBarText": "TitleBar",
        "titleBar": false,
        "menu": false,
        "titleBarBackgroundColor": "#878787",
        "fullScreen": true
    }
}